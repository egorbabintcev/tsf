{
  "extends": [
    "@tsconfig/node22/tsconfig.json",
    "@tsconfig/node-ts/tsconfig.json"
  ],
  "include": [
    "*.config.ts",
    "*.config.js"
  ],
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "types": [
      "node"
    ],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsbuild.node.info"
  }
}